<Application
    x:Class="VariableSizedGridViewSample.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:selector="using:VariableSizedGridViewSample.Selectors"
    xmlns:local="using:VariableSizedGridViewSample"
    xmlns:localData="using:VariableSizedGridViewSample.Data">

    <Application.Resources>

        <!--アプリケーション固有のリソース -->

        <x:String x:Key="AppName">Variable Sized GridView Sample</x:String>

        <SolidColorBrush x:Key="ButtonPressedBackgroundThemeBrush" Color="#FF3D3D3D"/>
        <SolidColorBrush x:Key="ButtonPressedForegroundThemeBrush" Color="#FFFFFFFF"/>

    </Application.Resources>
</Application>
